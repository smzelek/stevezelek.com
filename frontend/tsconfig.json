{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "lib": [
            "DOM",
            "DOM.Iterable",
            "esnext"
        ],
        "jsx": "preserve",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "paths": {
            "~core": [
                "../core"
            ],
            "~backend*": [
                "../backend/src*"
            ],
            "~frontend*": [
                "./src*"
            ]
        }
    },
    "include": [
        "src",
    ]
}